{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react/jsx-runtime"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "settings": {
    "react": {
      "version": "19.1"
    }
  },
  "rules": {
    "indent": ["error", 4],
    "semi": ["error", "always"],
    "quotes": ["error", "single"],
    "react/jsx-key": "error"
  }
}